<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<!--=========================================================================================================-->
	<xs:complexType name="tgGAEB">
		<xs:annotation>
			<xs:documentation>Aufgestellt vom Gemeinsamen Ausschuss Elektronik im Bauwesen (GAEB) © 2002 - 2013 by DIN Deutsches Institut fuer Normung e. V.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="GAEBInfo" type="tgGAEBInfo">
				<xs:annotation>
					<xs:documentation>Informationen zum GAEB-Datenaustausch</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="PrjInfo" type="tgPrjInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Informationen zum Projekt</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Award" type="tgAward" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Vergabe
In diesem Objekt werden die Informationen des Vergabeverfahrens abgelegt. 
Das Objekt kann in den Datenaustauschphasen D81-D88 verwendet werden.</xs:documentation>
				</xs:annotation>
				<!--
					<xs:unique name="Constraint_Description_ID">
						<xs:selector xpath=".//Description | PreBoQText | PostBoQText"/>
						<xs:field xpath="@ID"/>
					</xs:unique>
					-->
			</xs:element>
			<xs:element name="Invoice" type="tgInvoice" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Rechnung
In diesem Objekt werden die detaillierten Informationen zur Rechnung abgelegt.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Order" type="tgOrder" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Bestellung  Datenphase D9x. Die Wiederholung ist nur im Kontext der Rechnung möglich. 
Hier wird in der ersten Wiederholung die Rechnung und in den weiteren Wiederholungen die einzelnen Lieferscheine übertragen
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AddText" type="tgAddText" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Zusaetzlicher Text; Zusätzliche Textinformation können hier abgelegt werden. </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!--/ GAEB-->
	<!--=========================================================================================================-->
	<xs:complexType name="tgGAEBInfo">
		<xs:annotation>
			<xs:documentation>Informationen zum GAEB-Datenaustausch</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Version" type="tgVersion">
				<xs:annotation>
					<xs:documentation>Version des GAEB DA XML</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="VersDate" type="tgGAEBVersDate">
				<xs:annotation>
					<xs:documentation>Datum der Ausgabe der GAEB-DA-XML  Schnittstellendefinition</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Date" type="tgDate">
				<xs:annotation>
					<xs:documentation>Datum</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Time" type="tgTime" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Erstellungszeit der Datenaustauschdatei</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProgSystem" type="tgNormalizedString60" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Programmsystem</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProgName" type="tgNormalizedString60" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Umsetzprogramm</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Certific" type="tgCertific" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Pruefzertifikat der benutzten Version GAEB DA XML liegt vor</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="tgVersion">
		<xs:restriction base="tgNormalizedString10">
			<xs:enumeration value="3.2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tgCertific">
		<xs:restriction base="tgNormalizedString">
			<xs:enumeration value="Available"/>
		</xs:restriction>
	</xs:simpleType>
