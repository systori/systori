         <Item ID="DBLEBBDI" RNoPart="1">
          <LumpSumItem>Yes</LumpSumItem>
          <Qty>1.000</Qty>
          <QU>psch</QU>
          <Description ID="R20140530091043775000007">
           <CompleteText>
            <DetailTxt>
             <Text>
              <p style="text-align:left;margin-top:0pt;margin-bottom:0pt;">
               <span style="font-family:Arial;font-size:10pt;Color:rgb(0,0,0);">Gefälleplanung</span>
              </p>
              <p style="text-align:left;margin-top:0pt;margin-bottom:0pt;">
               <span style="font-family:Arial;font-size:10pt;Color:rgb(0,0,0);">Erstellen der Gefälleplanung für alle im Leistungsverzeichnis</span>
              </p>
